# API Configuration
# Copy this file to .env and fill in your actual API keys

# Twilio Configuration
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
VITE_TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# ElevenLabs Configuration (Portuguese Voice)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
VITE_ELEVENLABS_VOICE_ID=your_portuguese_voice_id_here

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Alternative: Claude API Configuration
VITE_CLAUDE_API_KEY=your_claude_api_key_here

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
VITE_APP_NAME=AI Call Assistant
VITE_APP_VERSION=1.0.0

# AI Configuration
VITE_AI_PROVIDER=openai
VITE_AI_MODEL=gpt-4
VITE_AI_TEMPERATURE=0.7
VITE_AI_MAX_TOKENS=150

# Voice Configuration
VITE_VOICE_LANGUAGE=pt-BR
VITE_VOICE_SPEED=1.0
VITE_VOICE_PITCH=0.0
VITE_VOICE_CLARITY=high
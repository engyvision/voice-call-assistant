# API Configuration
# Copy this file to .env and fill in your actual API keys

# Telnyx Configuration (NEW - replaces Twilio)
VITE_TELNYX_API_KEY=your_telnyx_api_key_here
VITE_TELNYX_CONNECTION_ID=your_telnyx_connection_id_here
VITE_TELNYX_PHONE_NUMBER=+351210600099

# AI Configuration (UPDATED)
VITE_AI_PROVIDER=openai
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_AI_MODEL=gpt-4o
VITE_AI_TEMPERATURE=0.7
VITE_AI_MAX_TOKENS=150

# Voice Configuration (UPDATED - AWS Polly)
VITE_TTS_ENGINE=aws.polly
VITE_TTS_VOICE=Joanna-Neural
VITE_VOICE_LANGUAGE=en-US
VITE_VOICE_SPEED=1.0
VITE_VOICE_PITCH=0.0

# Transcription Configuration (NEW)
VITE_TRANSCRIPTION_MODEL=distil-whisper/distil-large-v2

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
VITE_APP_NAME=AI Call Assistant
VITE_APP_VERSION=1.0.0

# Development Tools (optional)
VITE_SHOW_DEBUG_TABS=true  # Show debug tools in production